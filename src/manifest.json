{
  "manifest_version": 2,

  "name": "Easy URL Params",
  "description": "Easy URL Params is an extension for Chrome to manipulate and visualize the query parameters in URL",
  "version": "1.3.1",
  "icons": {
	"16": "img/icon/icon16.png",
	"48": "img/icon/icon48.png",
	"128": "img/icon/icon128.png"
  },
  "browser_action": {
    "default_icon": {
		"19": "img/icon/icon19.png",
		"38": "img/icon/icon38.png"
	},
	"default_title": "Easy URL Params",
    "default_popup": "app/app.html"
  },
  "permissions": [
    "tabs"
  ]
}